<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane prefHeight="462.0" prefWidth="1628.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.FXHelloCVController">
    <bottom>
        <HBox alignment="center" prefHeight="0.0" prefWidth="1628.0">
            <padding>
                <Insets bottom="25" left="25" right="25" top="25" />
            </padding>
         <Label fx:id="label1" text="Label" />
         <Slider fx:id="slider1" max="255.0" showTickLabels="true" showTickMarks="true" />
            <Button fx:id="button" alignment="center" onAction="#startCamera" prefHeight="25.0" prefWidth="128.0" text="Start Camera" />
         <Slider fx:id="slider2" max="255.0" showTickLabels="true" showTickMarks="true" />
         <Label fx:id="label2" text="Label" />
        </HBox>
    </bottom>
    <center>
        <ImageView fx:id="currentFrame" />
    </center>
   <right>
      <ImageView fx:id="processedFrame" fitHeight="390.0" fitWidth="563.0" pickOnBounds="true" preserveRatio="true" BorderPane.alignment="CENTER" />
   </right>
</BorderPane>
